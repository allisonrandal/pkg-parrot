#! perl -w
# $Id: schemec 7955 2005-05-02 22:31:55Z bernhard $

use strict;
use lib 'languages/scheme';
use Scheme;

sub Usage {
  print <<_EOF_;
usage: $0 <file.scheme>
_EOF_
  exit;
}

defined $ARGV[0] or Usage();
$ARGV[0] =~ m/\.scheme$|\.scm$/i or Usage();

print Scheme->new($ARGV[0])->compile();
