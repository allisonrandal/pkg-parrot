#include <stdio.h>
#include <stdlib.h>
#include <pcre.h>

int
main(int argc, char *argv[])
{

    if (pcre_exec == NULL) {
        printf("pcre_exec is NULL\n");
        return EXIT_FAILURE;
    }
    printf("pcre %s\n", pcre_version());

    return EXIT_SUCCESS;
}

